<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.nba.bean.Team_honor" table="team_honor">
    
        <composite-id>
            <key-property name="team_id" column="team_id" type="int"></key-property>
            <key-property name="honor_name" column="honor_name" type="string"></key-property>
            <key-property name="season" column="season" type="string"></key-property>
        </composite-id>
    
    </class>

</hibernate-mapping>